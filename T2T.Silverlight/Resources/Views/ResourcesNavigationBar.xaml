<UserControl x:Class="T2T.Resources.Views.ResourcesNavigationBar"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">
    
    <Grid x:Name="LayoutRoot" Background="White">
        <StackPanel    Orientation="Horizontal" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" x:Name="navigationBar">
            <HyperlinkButton x:Name="Link2"   NavigateUri="/Resources/MaterialsWorkspace"   Content="MATERIALS" Style="{StaticResource SmallLinkStyle}"   />
            <HyperlinkButton x:Name="Link3"   NavigateUri="/Resources/EquipmentWorkspace"  Content="EQUIPMENT" Style="{StaticResource SmallLinkStyle}"/>
            <HyperlinkButton x:Name="Link4"   NavigateUri="/Resources/PersonnelWorkspace" Content="PERSONELL" Style="{StaticResource SmallLinkStyle}"/>
            <HyperlinkButton x:Name="Link5"   NavigateUri="/Resources/PhysicalAssetWorkspace"  Content="PHYSICAL ASSETS" Style="{StaticResource SmallLinkStyle}"/>
            <HyperlinkButton x:Name="Link6"   NavigateUri="/Resources/ProcessCellWorkspace" Content="PROCESS CELLS" Style="{StaticResource SmallLinkStyle}"/>
        </StackPanel>
    </Grid>
</UserControl>
